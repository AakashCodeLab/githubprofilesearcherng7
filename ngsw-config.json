{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/girl.png",
          "/index.html",
          "/assets/**",
          "/*.css",
          "/*.js"

        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://maxcdn.bootstrapcdn.com/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "from-api",
      "urls": ["http://api.github.com/users/"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 15,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }
  ]
}
